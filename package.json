{
  "name": "react-functional-select",
  "version": "1.0.0-beta.0",
  "author": "Matt Areddia <mareddia@protonmail.com> (https://github.com/based-ghost)",
  "description": "A high-performance, low-code select control for React",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "typings": "dist/es/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/based-ghost/react-functional-select/issues"
  },
  "repository": {
    "url": "https://github.com/based-ghost/react-functional-select.git",
    "type": "git"
  },
  "homepage": "https://github.com/based-ghost/react-functional-select#readme",
  "keywords": [
    "react",
    "reactjs",
    "react-component",
    "react-virtualized",
    "select",
    "react-select",
    "dropdown",
    "styled-components",
    "virtual",
    "virtualized",
    "window",
    "windowed",
    "react-window",
    "combobox",
    "form",
    "input",
    "ui"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-destructuring": "^7.6.0",
    "@babel/plugin-transform-parameters": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/plugin-transform-spread": "^7.6.2",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@storybook/addon-storysource": "^5.2.4",
    "@storybook/addons": "^5.2.4",
    "@storybook/react": "^5.2.4",
    "@testing-library/jest-dom": "^4.1.2",
    "@testing-library/react": "^9.3.0",
    "@types/jest": "^24.0.19",
    "@types/node": "^12.11.1",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "@types/react-window": "^1.8.1",
    "@types/storybook__react": "^4.0.2",
    "@types/styled-components": "^4.1.19",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-async-to-promises": "^0.8.14",
    "concurrently": "^4.1.1",
    "cross-env": "^5.2.0",
    "eslint": "^6.2.0",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "polished": "^3.4.1",
    "prepend-file": "^1.3.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-window": "^1.8.5",
    "styled-components": "^5.0.0-beta.8-groupsizefix",
    "typescript": "^3.5.3",
    "webpack": "^4.41.0"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-window": "^1.8.5",
    "styled-components": "^4.4.0",
    "polished": "^3.4.1",
    "@types/styled-components": "^4.1.19",
    "@types/react-window": "^1.8.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5"
  },
  "scripts": {
    "build": "run-p build:cjs build:es build:lib",
    "build:cjs": "cross-env BUILD_TYPE=cjs run-p build:cjs:*",
    "build:cjs:files": "babel -x .ts,.tsx,.js,.jsx src --out-dir dist/cjs --ignore \"**/*.d.ts\" --copy-files",
    "build:cjs:dts": "tsc -p tsconfig.prod.json --emitDeclarationOnly --outDir dist/cjs",
    "build:es": "run-p build:es:*",
    "build:es:files": "babel -x .ts,.tsx,.js,.jsx src --out-dir dist/es --ignore \"**/*.d.ts\" --copy-files",
    "build:es:dts": "tsc -p tsconfig.prod.json --emitDeclarationOnly --outDir dist/es",
    "build:lib": "cross-env BUILD_TYPE=lib run-p build:lib:*",
    "build:lib:files": "babel -x .ts,.tsx,.js,.jsx src --out-dir dist/lib --ignore \"**/*.d.ts\" --copy-files",
    "build:lib:dts": "tsc -p tsconfig.prod.json --emitDeclarationOnly --outDir dist/lib",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "cross-env NODE_ENV=test jest",
    "start": "run-s storybook",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o docs"
  }
}
